type: ipv4, ipv6
help: Black hole IP for dropping blacklisted hosts

val_help: ipv4net; IP address
val_help: ipv6net; IPv6 address

default: 0.0.0.0

create:expression: "touch /tmp/update-blacklists.$PPID"
delete:expression: "touch /tmp/update-blacklists.$PPID"
update:expression: "touch /tmp/update-blacklists.$PPID"
